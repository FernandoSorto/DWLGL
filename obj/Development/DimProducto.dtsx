                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    0                 AS producto_id,
    ''                AS codigo,
    ''                AS nombre,
    ''                AS categoria,
    ''                AS tipo,
    CAST(0.00 AS DECIMAL(8,2)) AS costo,
    0                 AS producto_activo
UNION ALL
SELECT 
    p.id              AS producto_id,
    p.codigo          AS codigo,
    p.nombre          AS nombre,
    c.nombre          AS categoria,
    t.nombre          AS tipo,
    CAST(p.costo AS DECIMAL(8,2)) AS costo,
    p.producto_activo AS producto_activo
FROM dbo.productos p
INNER JOIN dbo.categorias c 
    ON p.categoria_id = c.id
INNER JOIN dbo.tipo_productos t
    ON p.tipo_producto_id = t.id;</property>
